{
  "extends": ["airbnb", "airbnb/hooks", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-use-before-define": 0,
    "react/jsx-curly-newline": 0
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  }
}
